{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BaseAddress": "https://localhost:7157",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(localdb)\\MSSQLLocalDB;Database=VegetableDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Jwt": {
    "SecretKey": "0123456789ABCDEF",
    "Issuer": "https://localhost:7157",
    "Audience": "https://localhost:7157",
    "TokenValidityInMinutes": 10,
    "RefreshTokenValidityInDays": 7
  },
  "ValidateJwt": {
    "RequiredLengthPassword": 6,
    "DefaultLockoutTimeSpan": 5,
    "MaxFailedAccessAttempts": 3,
    "RequiredUniqueChars": 2
  }
}
